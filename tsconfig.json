{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "types": ["@modyfi/vite-plugin-yaml/modules"],
    "jsx": "react",
    "baseUrl": ".",
    "paths": {
      "@public/*": ["public/*"],
      "@components/*": ["src/components/*"],
      "@layouts/*": ["src/layouts/*"],
      "@styles/*": ["src/styles/*"],
      "@assets/*": ["src/assets/*"]
    }
  }
}
