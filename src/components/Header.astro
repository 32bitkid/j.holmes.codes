<style>
  :global(:root) {
    padding-top: calc(var(--line-height) * 1.5);
  }

  header {
    border-bottom: 1px dotted #ddd;
    position: fixed;
    top: 0;
    left: 0;
    right: 0;
    background-color: white;
    z-index: 1;
    height: calc(var(--line-height) * 1.5);
  }

  @media print {
    header {
      display: none;
    }
  }

  nav {
    max-width: var(--max-content);
    margin-left: auto;
    margin-right: auto;
    display: flex;
    gap: 0.25ch;
    align-items: center;
    padding-inline: 1.5ch;
  }

  .spacer {
    flex-grow: 1;
  }

  a.icon-link {
    font-size: 0;
    line-height: 0;
  }

  a.icon-link::after {
    display: block;
    width: 2rem;
    height: 2rem;
    box-sizing: border-box;
    opacity: 0.25;
    filter: grayscale(100%) brightness(0);
    transform: scale(0.8);
    transition:
      filter 500ms,
      opacity 500ms,
      transform 250ms ease-in;
  }

  a.icon-link:hover::after {
    opacity: 1;
    filter: grayscale(0%);
    transform: scale(1);
  }

  a.icon-link.codepen::after {
    content: url(data:image/svg+xml;base64,);
  }

  a.icon-link.github::after {
    content: url(data:image/svg+xml;base64,);
  }

  a.icon-link.stack-overflow::after {
    content: url(data:image/svg+xml;base64,);
  }

  a.icon-link.bsky::after {
    content: url(data:image/svg+xml;base64,);
  }

  a.icon-link.linked-in::after {
    content: url(data:image/svg+xml;base64,);
  }

  a.icon-link.mstdn::after {
    content: url(data:image/svg+xml;base64,);
  }
</style>

